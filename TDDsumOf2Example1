import junit.framework.TestCase;

public class Test extends TestCase {

	TDDCalculator testObject;
	
	public void setUp () {
		
		testObject = new TDDCalculator();
		
	}
	
	public void testObject () throws Exception {
		
		assertNotNull (testObject);
		
	}
	
	public void testAddition_True () throws Exception {
		
		int a =2;
		int b= 2;
		testObject.add2Val(a,b);
		
	}
	
	public void testAddition_True2 () throws Exception {
		
		int a =5;
		int b= 2;
		testObject.add2Val(a,b);
		
	}
	
	
	
}
