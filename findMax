import java.util.Scanner;

public class findMax {
	public static int getMax(int [] number) {
		int max = number[0];
		for(int i=1; i<number.length; i++) {
		
			if(number[i] > max ) {
				max = number[i];
			}
		}
		return max;
	}
	
	public static void main(String[] args) {
		int number[] = {1 , 2 , 3 };
		System.out.println("Max = " + getMax(number));
	}
}
